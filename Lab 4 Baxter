#Data Set A


RECODE sex ('female'='1') ('male'='0').
EXECUTE.


MIXED pain WITH sex age STAI_trait pain_cat cortisol_serum mindfulness
  /CRITERIA=DFMETHOD(SATTERTHWAITE) CIN(95) MXITER(100) MXSTEP(10) SCORING(1) 
    SINGULAR(0.000000000001) HCONVERGE(0, ABSOLUTE) LCONVERGE(0, ABSOLUTE) PCONVERGE(0.000001, ABSOLUTE)    
  /FIXED=sex age STAI_trait pain_cat cortisol_serum mindfulness | SSTYPE(3)
  /METHOD=REML
  /PRINT=DESCRIPTIVES  SOLUTION
  /RANDOM=INTERCEPT | SUBJECT(hospital) COVTYPE(VC).


MIXED pain WITH sex age STAI_trait pain_cat cortisol_serum mindfulness
  /CRITERIA=DFMETHOD(SATTERTHWAITE) CIN(95) MXITER(100) MXSTEP(10) SCORING(1) 
    SINGULAR(0.000000000001) HCONVERGE(0, ABSOLUTE) LCONVERGE(0, ABSOLUTE) PCONVERGE(0.000001, ABSOLUTE)    
  /FIXED=sex age STAI_trait pain_cat cortisol_serum mindfulness | SSTYPE(3)
  /METHOD=REML
  /PRINT=DESCRIPTIVES  SOLUTION
  /RANDOM=INTERCEPT | SUBJECT(hospital) COVTYPE(VC)
  /SAVE=PRED.

DESCRIPTIVES VARIABLES=pred_pain_int
  /STATISTICS=MEAN STDDEV MIN MAX.
  
  
  #Data Set B
  
  * Encoding: UTF-8.

DATASET ACTIVATE DataSet6.
RECODE sex ('female'='1') ('male'='0').
EXECUTE.

COMPUTE pred_pain=2.762749 + (-0.200128 * sex) + (-0.022589 * age) + (-0.046043 * STAI_trait) +( 
    0.081194 * pain_cat) + (-0.183630 * mindfulness) + (0.626308
 * cortisol_serum).
EXECUTE.

REGRESSION
  /MISSING LISTWISE
  /STATISTICS COEFF OUTS R ANOVA
  /CRITERIA=PIN(.05) POUT(.10)
  /NOORIGIN 
  /DEPENDENT pain
  /METHOD=ENTER sex age STAI_trait pain_cat cortisol_serum mindfulness
  /SAVE RESID.

REGRESSION
  /MISSING LISTWISE
  /STATISTICS COEFF OUTS R ANOVA
  /CRITERIA=PIN(.05) POUT(.10)
  /NOORIGIN 
  /DEPENDENT pain
  /METHOD=ENTER sex age STAI_trait pain_cat cortisol_serum mindfulness
  /SAVE PRED.

COMPUTE res_sq=RES_1 * RES_1.
EXECUTE.

DESCRIPTIVES VARIABLES=res_sq
  /STATISTICS=MEAN SUM STDDEV.

DESCRIPTIVES VARIABLES=pred_pain
  /STATISTICS=MEAN STDDEV MIN MAX.

COMPUTE pred_mean=39.571429. 
EXECUTE.

COMPUTE res_mean=pain - pred_mean.
EXECUTE.

COMPUTE res_mean_sq=res_mean * res_mean.
EXECUTE.

DESCRIPTIVES VARIABLES=res_mean_sq
  /STATISTICS=MEAN SUM STDDEV MIN MAX.

COMPUTE R_sq = 1-(272.27 / 429.07). 
EXECUTE.
