DATASET ACTIVATE DataSet1.
RECODE Sex ('female'='1') ('male'='0').
EXECUTE.

RECODE SibSp (0=0) (1 thru 8=1).
EXECUTE.

RECODE Parch (2 thru 14=1) (ELSE=0).
EXECUTE.

RECODE Embarked ('S'=0) ('C'=1) ('Q'=2) INTO embarked_numeric.
EXECUTE.

RECODE embarked_numeric (0=0) (1 thru 2=1) INTO embark_sh.
EXECUTE.

FREQUENCIES VARIABLES=Survived Age sex SibSp Parch Pclass
  /BARCHART FREQ
  /ORDER=ANALYSIS.

EXAMINE VARIABLES=Survived BY Pclass Age SibSp Parch sex
  /PLOT BOXPLOT HISTOGRAM
  /COMPARE GROUPS
  /STATISTICS DESCRIPTIVES
  /CINTERVAL 95
  /MISSING LISTWISE
  /NOTOTAL.


CROSSTABS
  /TABLES=Survived BY Pclass Sex Age SibSp Parch
  /FORMAT=AVALUE TABLES
  /CELLS=COUNT ROW 
  /COUNT ROUND CELL.

CROSSTABS
  /TABLES=Survived BY Pclass Sex Age SibSp Parch embark_sh
  /FORMAT=AVALUE TABLES
  /CELLS=COUNT ROW 
  /COUNT ROUND CELL.

RECODE Pclass (2=1) (ELSE=0) INTO secondclass.
EXECUTE.

RECODE Pclass (1=1) (ELSE=0) INTO firstclass.
EXECUTE.

LOGISTIC REGRESSION VARIABLES Survived
  /METHOD=ENTER firstclass embark_sh Parch SibSp Sex Age
  /CONTRAST (Sex)=Indicator
  /SAVE=PRED PGROUP
  /CRITERIA=PIN(.05) POUT(.10) ITERATE(20) CUT(.5).

LOGISTIC REGRESSION VARIABLES Survived
  /METHOD=ENTER Sex Sibsp Parch firstclass secondclass Age 
  /CONTRAST (Sex)=Indicator
  /SAVE=PRED PGROUP RESID
  /CRITERIA=PIN(.05) POUT(.10) ITERATE(20) CUT(.5).

LOGISTIC REGRESSION VARIABLES Survived
  /METHOD=ENTER Sex SibSp firstclass secondclass age
  /CLASSPLOT
  /PRINT=CORR
  /CRITERIA=PIN(0.05) POUT(0.10) ITERATE(20) CUT(0.5).

NOMREG Survived (BASE=FIRST ORDER=ASCENDING) WITH Sex Sibsp Parch firstclass secondclass age
  /CRITERIA CIN(95) DELTA(0) MXITER(100) MXSTEP(5) CHKSEP(20) LCONVERGE(0) PCONVERGE(0.000001) 
    SINGULAR(0.00000001)
  /FULLFACTORIAL
  /STEPWISE=PIN(.05) POUT(0.1) MINEFFECT(0) RULE(SINGLE) ENTRYMETHOD(LR) REMOVALMETHOD(LR)
  /INTERCEPT=INCLUDE
  /PRINT=CLASSTABLE PARAMETER SUMMARY LRT CPS STEP MFI IC.


